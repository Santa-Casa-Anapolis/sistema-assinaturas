<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="bpmn-js (https://demo.bpmn.io)" exporterVersion="9.0.3">
  <bpmn:process id="Process_1" isExecutable="false">
    <bpmn:startEvent id="StartEvent_1" name="Fornecedor Envia Nota Fiscal">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:userTask id="UserTask_1" name="Upload do Documento">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
      <bpmn:documentation>Fornecedor faz upload da nota fiscal com assinatura GOV.BR</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_1" name="Documento Válido?">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rejeitar</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="UserTask_2" name="Supervisor Analisa e Assina">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
      <bpmn:documentation>Supervisor do setor responsável analisa e assina via GOV.BR</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_2" name="Supervisor Aprovou?">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
      <bpmn:outgoing>Flow_RejeitarSupervisor</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="UserTask_3" name="Contabilidade Analisa e Assina">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:documentation>Departamento de contabilidade analisa e assina</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_3" name="Contabilidade Aprovou?">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
      <bpmn:outgoing>Flow_RejeitarContabilidade</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="UserTask_4" name="Financeiro Analisa e Assina">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
      <bpmn:documentation>Departamento financeiro analisa e assina</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_4" name="Financeiro Aprovou?">
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
      <bpmn:outgoing>Flow_RejeitarFinanceiro</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:userTask id="UserTask_5" name="Diretoria Assina Final">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
      <bpmn:documentation>Diretoria faz assinatura final via GOV.BR</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:exclusiveGateway id="Gateway_5" name="Diretoria Aprovou?">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
      <bpmn:outgoing>Flow_RejeitarDiretoria</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:serviceTask id="ServiceTask_1" name="Mover para Pasta de Rede">
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
      <bpmn:documentation>Sistema move documento para pasta organizada por setor/ano/mês</bpmn:documentation>
    </bpmn:serviceTask>
    
    <bpmn:serviceTask id="ServiceTask_2" name="Registrar Auditoria">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
      <bpmn:documentation>Sistema registra todas as ações no log de auditoria</bpmn:documentation>
    </bpmn:serviceTask>
    
    <bpmn:endEvent id="EndEvent_1" name="Processo Concluído">
      <bpmn:incoming>Flow_13</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Fluxos de Rejeição -->
    <bpmn:userTask id="UserTask_Rejeitar" name="Notificar Rejeição">
      <bpmn:incoming>Flow_Rejeitar</bpmn:incoming>
      <bpmn:outgoing>Flow_EndRejeitar</bpmn:outgoing>
      <bpmn:documentation>Notificar fornecedor sobre rejeição do documento</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_Rejeitar" name="Processo Rejeitado">
      <bpmn:incoming>Flow_EndRejeitar</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Fluxos de Rejeição por Setor -->
    <bpmn:userTask id="UserTask_RejeitarSupervisor" name="Supervisor Rejeita">
      <bpmn:incoming>Flow_RejeitarSupervisor</bpmn:incoming>
      <bpmn:outgoing>Flow_EndRejeitarSupervisor</bpmn:outgoing>
      <bpmn:documentation>Supervisor rejeita documento com justificativa</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_RejeitarSupervisor" name="Rejeitado pelo Supervisor">
      <bpmn:incoming>Flow_EndRejeitarSupervisor</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:userTask id="UserTask_RejeitarContabilidade" name="Contabilidade Rejeita">
      <bpmn:incoming>Flow_RejeitarContabilidade</bpmn:incoming>
      <bpmn:outgoing>Flow_EndRejeitarContabilidade</bpmn:outgoing>
      <bpmn:documentation>Contabilidade rejeita documento com justificativa</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_RejeitarContabilidade" name="Rejeitado pela Contabilidade">
      <bpmn:incoming>Flow_EndRejeitarContabilidade</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:userTask id="UserTask_RejeitarFinanceiro" name="Financeiro Rejeita">
      <bpmn:incoming>Flow_RejeitarFinanceiro</bpmn:incoming>
      <bpmn:outgoing>Flow_EndRejeitarFinanceiro</bpmn:outgoing>
      <bpmn:documentation>Financeiro rejeita documento com justificativa</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_RejeitarFinanceiro" name="Rejeitado pelo Financeiro">
      <bpmn:incoming>Flow_EndRejeitarFinanceiro</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:userTask id="UserTask_RejeitarDiretoria" name="Diretoria Rejeita">
      <bpmn:incoming>Flow_RejeitarDiretoria</bpmn:incoming>
      <bpmn:outgoing>Flow_EndRejeitarDiretoria</bpmn:outgoing>
      <bpmn:documentation>Diretoria rejeita documento com justificativa</bpmn:documentation>
    </bpmn:userTask>
    
    <bpmn:endEvent id="EndEvent_RejeitarDiretoria" name="Rejeitado pela Diretoria">
      <bpmn:incoming>Flow_EndRejeitarDiretoria</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Fluxos de Conexão -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="UserTask_1" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="UserTask_1" targetRef="Gateway_1" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Gateway_1" targetRef="UserTask_2" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="UserTask_2" targetRef="Gateway_2" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_2" targetRef="UserTask_3" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="UserTask_3" targetRef="Gateway_3" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Gateway_3" targetRef="UserTask_4" />
    <bpmn:sequenceFlow id="Flow_8" sourceRef="UserTask_4" targetRef="Gateway_4" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Gateway_4" targetRef="UserTask_5" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="UserTask_5" targetRef="Gateway_5" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Gateway_5" targetRef="ServiceTask_1" />
    <bpmn:sequenceFlow id="Flow_12" sourceRef="ServiceTask_1" targetRef="ServiceTask_2" />
    <bpmn:sequenceFlow id="Flow_13" sourceRef="ServiceTask_2" targetRef="EndEvent_1" />
    
    <!-- Fluxos de Rejeição -->
    <bpmn:sequenceFlow id="Flow_Rejeitar" sourceRef="Gateway_1" targetRef="UserTask_Rejeitar" />
    <bpmn:sequenceFlow id="Flow_EndRejeitar" sourceRef="UserTask_Rejeitar" targetRef="EndEvent_Rejeitar" />
    <bpmn:sequenceFlow id="Flow_RejeitarSupervisor" sourceRef="Gateway_2" targetRef="UserTask_RejeitarSupervisor" />
    <bpmn:sequenceFlow id="Flow_EndRejeitarSupervisor" sourceRef="UserTask_RejeitarSupervisor" targetRef="EndEvent_RejeitarSupervisor" />
    <bpmn:sequenceFlow id="Flow_RejeitarContabilidade" sourceRef="Gateway_3" targetRef="UserTask_RejeitarContabilidade" />
    <bpmn:sequenceFlow id="Flow_EndRejeitarContabilidade" sourceRef="UserTask_RejeitarContabilidade" targetRef="EndEvent_RejeitarContabilidade" />
    <bpmn:sequenceFlow id="Flow_RejeitarFinanceiro" sourceRef="Gateway_4" targetRef="UserTask_RejeitarFinanceiro" />
    <bpmn:sequenceFlow id="Flow_EndRejeitarFinanceiro" sourceRef="UserTask_RejeitarFinanceiro" targetRef="EndEvent_RejeitarFinanceiro" />
    <bpmn:sequenceFlow id="Flow_RejeitarDiretoria" sourceRef="Gateway_5" targetRef="UserTask_RejeitarDiretoria" />
    <bpmn:sequenceFlow id="Flow_EndRejeitarDiretoria" sourceRef="UserTask_RejeitarDiretoria" targetRef="EndEvent_RejeitarDiretoria" />
  </bpmn:process>
  
